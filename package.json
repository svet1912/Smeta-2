{
  "name": "mantis-material-free-react",
  "version": "1.6.0",
  "homepage": "https://mantisdashboard.com/free",
  "author": {
    "name": "CodedThemes",
    "email": "codedthemes@gmail.com",
    "url": "https://codedthemes.com/"
  },
  "scripts": {
    "start": "vite",
    "build": "npm run generate:robots && vite build",
    "build:web": "npm run generate:robots && vite build",
    "build:vercel": "npm run generate:robots && vite build",
    "preview": "vite preview",
    "serve:web:preview": "vite preview --host --strictPort --port 4174",
    "generate:robots": "node scripts/generate-robots.js",
    "serve:api": "node server/index.js",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && node start.js",
    "client": "vite",
    "start:all": "concurrently --names \"BACKEND,FRONTEND\" --prefix-colors \"bgBlue,bgGreen\" \"npm run server\" \"npm run client\"",
    "stop": "node scripts/stop-servers.js",
    "restart": "npm run stop && npm run start:all",
    "clean-start": "npm run stop && npm run start:all",
    "test": "node scripts/test-with-server.mjs",
    "test:backend": "vitest run --config vitest.config.js",
    "test:backend:ui": "vitest --config vitest.config.js",
    "test:backend:watch": "vitest --watch --config vitest.config.js",
    "test:backend:only": "vitest run --config vitest.config.js",
    "e2e:prep": "npm run build:web",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "test:e2e": "node scripts/test-e2e-with-app.mjs",
    "e2e:headed": "playwright test --headed",
    "e2e:report": "playwright show-report",
    "perf:api": "node tools/perf/api-bench.mjs",
    "perf:api:health": "PERF_URL=http://localhost:3001/api/health npm run perf:api",
    "perf:api:materials": "PERF_URL=http://localhost:3001/api/materials?limit=50 npm run perf:api",
    "perf:api:works": "PERF_URL=http://localhost:3001/api/works?limit=50 npm run perf:api",
    "lhci": "lhci autorun",
    "test:contracts": "node scripts/test-with-server.mjs test:contracts:only",
    "test:contracts:only": "vitest run tests/backend/contracts.test.ts --reporter=verbose",
    "test:cache": "node scripts/test-with-server.mjs test:cache:only",
    "test:cache:only": "vitest run tests/backend/cache-headers.test.ts --reporter=verbose",
    "test:perf": "npm run perf:api:health && npm run perf:api:materials",
    "ci:test": "npm run test && npm run test:contracts && npm run test:cache",
    "ci:perf": "npm run test:perf",
    "ci:web": "npm run lhci"
  },
  "dependencies": {
    "@ant-design/colors": "8.0.0",
    "@ant-design/icons": "6.0.0",
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@fontsource/inter": "5.2.6",
    "@fontsource/poppins": "5.2.6",
    "@fontsource/public-sans": "5.2.6",
    "@fontsource/roboto": "5.2.6",
    "@mui/base": "5.0.0-beta.70",
    "@mui/icons-material": "^7.3.2",
    "@mui/lab": "7.0.0-beta.10",
    "@mui/material": "^7.3.2",
    "@mui/system": "^7.3.2",
    "@mui/x-charts": "8.1.0",
    "@mui/x-date-pickers": "^8.12.0",
    "@tanstack/react-query": "^5.90.2",
    "@vitejs/plugin-react": "4.6.0",
    "antd": "^5.27.4",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "formik": "2.4.6",
    "framer-motion": "12.8.2",
    "hoist-non-react-statics": "^3.3.2",
    "lodash-es": "4.17.21",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-device-detect": "2.2.3",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-number-format": "5.4.4",
    "react-router": "7.6.3",
    "react-router-dom": "7.6.3",
    "simplebar-react": "3.3.2",
    "swr": "2.3.4",
    "vite": "7.0.4",
    "vite-jsconfig-paths": "2.0.1",
    "web-vitals": "4.2.4",
    "yup": "1.6.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/compat": "1.3.1",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.31.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.55.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/supertest": "^6.0.3",
    "autocannon": "^8.0.0",
    "concurrently": "^9.2.1",
    "eslint": "9.25.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "jsdom": "^27.0.0",
    "node-fetch": "^3.3.2",
    "prettier": "3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "supertest": "^7.1.4",
    "vitest": "^3.2.4",
    "wait-on": "^9.0.1",
    "whatwg-fetch": "^3.6.20",
    "zod": "^4.1.11"
  },
  "packageManager": "yarn@4.9.1"
}
